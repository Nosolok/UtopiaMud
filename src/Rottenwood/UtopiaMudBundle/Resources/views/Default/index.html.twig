{% extends 'RottenwoodUtopiaMudBundle:Default:layout.html.twig' %}

{% block content %}
    <div id="header"></div>
    {% block mapdiv %}
        <div id="map" class="minimap">
            <div id="nwwn">nwwn</div>
            <div id="nwn">nwn</div>
            <div id="nn">nn</div>
            <div id="nen">nen</div>
            <div id="neen">neen</div><br>
            <div id="nww">nww</div>
            <div id="nw">nw</div>
            <div id="n">n</div>
            <div id="ne">ne</div>
            <div id="nee">nee</div><br>
            <div id="ww">ww</div>
            <div id="w">w</div>
            <div id="r">r</div>
            <div id="e">e</div>
            <div id="ee">ee</div><br>
            <div id="sww">sww</div>
            <div id="sw">sw</div>
            <div id="s">s</div>
            <div id="se">se</div>
            <div id="see">see</div><br>
            <div id="swws">swws</div>
            <div id="sws">sws</div>
            <div id="ss">ss</div>
            <div id="ses">ses</div>
            <div id="sees">sees</div><br>
        </div>
    {% endblock mapdiv %}
    <div id="{% block gamedivname %}game{% endblock gamedivname %}">
        {% block game %}
            {% block websocket %}
                <script type="text/javascript">var hash = '{{ hash }}';</script>
                <script type="text/javascript">var serverip = '{{ serverip }}';</script>
            {% endblock websocket %}
        {% endblock game %}
    </div>
    <div id="chat">
        {% block chat %}
            <form id="chatform">
                <input id="chatinput" autocomplete="off" name="chat" type="text">
            </form>
        {% endblock chat %}
    </div>
{% endblock content %}