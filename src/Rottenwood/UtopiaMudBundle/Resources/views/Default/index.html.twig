{% extends 'RottenwoodUtopiaMudBundle:Default:layout.html.twig' %}

{% block content %}
    <div id="header"></div>
    {% block mapdiv %}
        <div id="map"><img src="{{ asset('assets/img/game_map.png') }}"></div>
    {% endblock mapdiv %}
    <div id="{% block gamedivname %}game{% endblock gamedivname %}">
        {% block game %}
            {% block websocket %}
                <script type="text/javascript">var hash = '{{ hash }}';</script>
            {% endblock websocket %}
        {% endblock game %}
    </div>
    <div id="chat">
        {% block chat %}
            <form id="chatform">
                <input id="chatinput" autocomplete="off" name="chat" type="text">
            </form>
        {% endblock chat %}
    </div>
{% endblock content %}